{
  "name": "@cheqd/connector-telegram",
  "version": "1.0.2-develop.6",
  "description": "Telegram OAuth connector for LogTo",
  "author": "Cheqd Foundation Limited (https://github.com/cheqd)",
  "license": "Apache-2.0",
  "repository": "https://github.com/cheqd/connector-telegram.git",
  "keywords": [
    "cheqd",
    "logto",
    "telegram",
    "oauth"
  ],
  "bugs": {
    "url": "https://github.com/cheqd/connector-telegram/issues"
  },
  "homepage": "https://github.com/cheqd/connector-telegram#readme",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "exports": "./lib/index.js",
  "type": "module",
  "files": [
    "lib",
    "config"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "build:test": "rm -rf lib/ && tsc -p tsconfig.test.json",
    "build": "rm -rf lib/ && tsc -p tsconfig.build.json --noEmit && rollup -c",
    "dev": "tsc -p tsconfig.build.json --watch --preserveWatchOutput --incremental",
    "lint": "eslint --ext .ts src",
    "lint:report": "pnpm lint --format json --output-file report.json",
    "test": "pnpm build:test && NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "pnpm run test --coverage --silent",
    "prepack": "pnpm build",
    "version": "pnpm package:update"
  },
  "dependencies": {
    "@logto/connector-kit": "1.1.0",
    "@silverhand/essentials": "2.6.1",
    "got": "^12.5.3",
    "snakecase-keys": "^5.1.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@silverhand/eslint-config": "3.0.1",
    "@silverhand/jest-config": "3.0.0",
    "@silverhand/ts-config": "^2.1.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.16.3",
    "@types/supertest": "^2.0.11",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.21.0",
    "jest": "^29.3.1",
    "jest-matcher-specific-error": "^1.0.0",
    "lint-staged": "^13.0.0",
    "nock": "^13.2.2",
    "prettier": "^2.8.1",
    "rollup": "^3.20.4",
    "rollup-plugin-summary": "^2.0.0",
    "semantic-release": "^21.0.1",
    "supertest": "^6.2.2",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": "^16.13.0 || ^18.12.0"
  },
  "eslintConfig": {
    "extends": "@silverhand"
  },
  "prettier": "@silverhand/eslint-config/.prettierrc",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
